<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.DarcekEntity" table="darcek">
        <id name="idDarceku">
            <column name="id_darceku" sql-type="int(11)"/>
        </id>
        <property name="idPredmetu">
            <column name="id_predmetu" sql-type="int(11)"/>
        </property>
        <property name="mnozstvo">
            <column name="mnozstvo" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="popis">
            <column name="popis" sql-type="varchar(2000)" length="2000" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>