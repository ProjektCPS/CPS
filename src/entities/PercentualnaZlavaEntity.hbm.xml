<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.PercentualnaZlavaEntity" table="percentualna_zlava">
        <id name="idPerZlavy">
            <column name="id_per_zlavy" sql-type="int(11)"/>
        </id>
        <property name="percentZlavy">
            <column name="percent_zlavy" sql-type="double" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>
