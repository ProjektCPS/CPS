<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.OsobaEntity" table="osoba" schema="sprava_cien_project">
        <id name="rodCislo">
            <column name="rod_cislo" sql-type="char(11)" length="11"/>
        </id>
        <property name="meno">
            <column name="meno" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="priezvisko">
            <column name="priezvisko" sql-type="varchar(30)" length="30"/>
        </property>
    </class>
</hibernate-mapping>