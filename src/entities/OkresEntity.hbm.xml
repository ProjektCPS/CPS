<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.OkresEntity" table="okres">
        <id name="idOkresu">
            <column name="id_okresu" sql-type="int(11)"/>
        </id>
        <property name="nazov">
            <column name="nazov" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="idKraja">
            <column name="id_kraja" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>
