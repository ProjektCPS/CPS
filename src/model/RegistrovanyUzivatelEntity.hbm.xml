<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.RegistrovanyUzivatelEntity" table="registrovany_uzivatel" schema="sprava_cien_project">
        <id name="idUzivatela">
            <column name="id_uzivatela" sql-type="int(11)"/>
        </id>
        <property name="email">
            <column name="email" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="heslo">
            <column name="heslo" sql-type="varchar(40)" length="40"/>
        </property>
    </class>
</hibernate-mapping>